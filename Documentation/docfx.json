{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            "DBFacade.Net/DBFacade/**.csproj"
          ],
          "cwd": "../../",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "GettingStarted/**.md",
          "Advanced/**.md",
          "Advanced/**/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ],
        "dest": ""
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "DbFacade",
      "_appLogoPath": "images/icon.jpg",
      "_appFooter": "Copyright &copy; JSystemsTech 2021",
      "_enableSearch": "true"
    },
    "postProcessors": [ "ExtractSearchIndex" ],
    "dest": "docs",
    "template": [ "templates/custom_default", "templates/material" ]
  }
}